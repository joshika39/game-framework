<Window x:Class="GameFramework.WPF.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:GameFramework.WPF.Views"
        xmlns:viewModels="clr-namespace:GameFramework.WPF.ViewModels"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800"
        d:DataContext="{d:DesignInstance {x:Type viewModels:IMainWindowViewModel}}">
    <Grid>
        <UserControl Content="{Binding MapView}" />
    </Grid>
</Window>